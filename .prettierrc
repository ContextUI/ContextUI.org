{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "strict",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss"]
}
